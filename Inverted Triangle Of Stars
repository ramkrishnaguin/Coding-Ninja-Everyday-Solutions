#include <bits/stdc++.h> 

vector<string> Triangle(int n)

{

   vector<string> v;

   for(int i =0;i<n;i++){

      string r="";

      int j;

      for(j=0;j<i;j++){

         r.push_back(' ');

      }

      for(int k =j;k<2*n-i-1;k++){

         r.push_back('*');

      }

      v.push_back(r);

   }

   return v;

}
